{
  "deno.enable": true,
  "deno.enablePaths": [
    "./components",
    "./lib",
    "./models",
    "./routes"
  ],
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.unstable": true,
  "search.useGlobalIgnoreFiles": true,
  "search.exclude": {
    "static/client.js": true
  },
  "explorer.excludeGitIgnore": false,
  "deno.path": "/usr/local/bin/deno",
}